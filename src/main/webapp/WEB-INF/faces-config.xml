<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config xmlns="http://java.sun.com/JSF/Configuration">

	<application>
		<locale-config>
			<default-locale>nb_NO</default-locale>
			<supported-locale>en_GB</supported-locale>
			<supported-locale>nb_NO</supported-locale>
		</locale-config>
		<message-bundle>Language_nb</message-bundle>
		<view-handler>no.uis.portal.util.LiferayViewHandler</view-handler>
		<variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
	</application>
	<factory>
		<faces-context-factory>com.icesoft.faces.context.FacesContextFactoryImpl</faces-context-factory>
	</factory>
	<navigation-rule>
		<navigation-case>
			<from-outcome>createAssignment</from-outcome>
			<to-view-id>/createAssignment.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignAssignment</from-outcome>
			<to-view-id>/assignAssignment.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>addExternalExaminer</from-outcome>
			<to-view-id>/addExternalExaminer.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>createAddressLabels</from-outcome>
			<to-view-id>/createAddressLabels.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editInformation</from-outcome>
			<to-view-id>/editInformation.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>myStudentAssignments</from-outcome>
			<to-view-id>/myStudentAssignments.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>searchArchive</from-outcome>
			<to-view-id>/searchArchive.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>backToView</from-outcome>
			<to-view-id>/view.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignmentSummary</from-outcome>
			<to-view-id>/assignmentSummary.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignmentSavedConfirmation</from-outcome>
			<to-view-id>/assignmentSavedConfirmation.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editDepartment</from-outcome>
			<to-view-id>/editDepartment.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editStudyPrograms</from-outcome>
			<to-view-id>/editStudyPrograms.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>displayAssignment</from-outcome>
			<to-view-id>/displayAssignment.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignAssignmentSummary</from-outcome>
			<to-view-id>/assignAssignmentSummary.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignSavedConfirmation</from-outcome>
			<to-view-id>/assignSavedConfirmation.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignSetDates</from-outcome>
			<to-view-id>/assignSetDates.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>assignmentAttachment</from-outcome>
			<to-view-id>/assignmentAttachment.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<managed-bean>
		<managed-bean-name>employeeService</managed-bean-name>
		<managed-bean-class>no.uis.portal.employee.EmployeeService</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>abamClient</property-name>
			<value>#{abamClient}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>assignmentBean</managed-bean-name>
		<managed-bean-class>no.uis.portal.employee.EmployeeAssignmentBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>employeeService</property-name>
			<value>#{employeeService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editBean</managed-bean-name>
		<managed-bean-class>no.uis.portal.employee.EditBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>employeeService</property-name>
			<value>#{employeeService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>assignSortableBean</managed-bean-name>
		<managed-bean-class>no.uis.portal.employee.AssignSortableBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>employeeService</property-name>
			<value>#{employeeService}</value>
		</managed-property>
	</managed-bean>
</faces-config>